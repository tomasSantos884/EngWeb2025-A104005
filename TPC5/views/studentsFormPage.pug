

block content 
    .w3-card-4
        .header.w3-container.w3-purple
            h2 Student Form    
        form(method="POST", action="/front/registo")
            fieldset
                legend Metadata
                label Id
                input.w3-input.w3-round(type="text" name="_id" required)
                label Name
                input.w3-input.w3-round(type="text" name="nome" required)
                label Git
                input.w3-input.w3-round(type="text" name="gitlink" required)

            fieldset
                legend TPC
                input.w3-check(type="checkbox" name="tpc1" value="1")
                label TPC1
                input.w3-check(type="checkbox" name="tpc2" value="1")
                label TPC2
                input.w3-check(type="checkbox" name="tpc3" value="1")
                label TPC3
                input.w3-check(type="checkbox" name="tpc4" value="1")
                label TPC4
                input.w3-check(type="checkbox" name="tpc5" value="1")
                label TPC5
                input.w3-check(type="checkbox" name="tpc6" value="1")
                label TPC6
                input.w3-check(type="checkbox" name="tpc7" value="1")
                label TPC7
                input.w3-check(type="checkbox" name="tpc8" value="1")
                label TPC8
            br
            button.w3-btn.w3-purple.w3-mb-2(type="submit") Register